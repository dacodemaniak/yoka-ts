{
    "name": "oop",
    "version": "1.0.0",
    "description": "Programmation OO avec Typescript",
    "main": "./dist/main.js",
    "scripts": {
        "babel": "babel --presets es2015 ./src/app.ts -o ./dist/main.js",
        "webpack:dev": "cp ./index.html ./dist && cp -R ./assets ./dist/  && cp ./sw.js ./dist && cp ./manifest.json ./dist/ && webpack --watch --mode development",
        "webpack:build": "webpack --mode production -p && cp ./index.html ./dist",
        "webpack:build:prod": "webpack --mode production -p && cp ./index.html ./dist && cp -R ./assets ./dist/ && cp ./sw.js ./dist && cp ./manifest.json ./dist/"
    },
    "keywords": [
        "Typescript",
        "OOP"
    ],
    "author": "IDea Factory",
    "license": "ISC",
    "devDependencies": {
        "@types/jquery": "^3.3.31",
        "@types/materialize-css": "^1.0.7",
        "babel-cli": "^6.26",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "css-loader": "^3.2.1",
        "cssnano": "^4.1.10",
        "mini-css-extract-plugin": "^0.8.0",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^4.0.0",
        "sass": "^1.23.7",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.1",
        "ts-loader": "^6.2.0",
        "typescript": "^3.6.4",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2"
    },
    "dependencies": {
        "bootstrap": "^4.4.1",
        "jquery": "^3.5.0",
        "material-design-icons": "^3.0.1",
        "materialize-css": "^1.0.0-rc.2"
    }
}
